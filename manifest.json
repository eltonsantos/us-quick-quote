{
  "manifest_version": 3,
  "name": "US Quick Quote",
  "version": "1.0.0",
  "description": "Quick NYSE/NASDAQ quotes + Top 3 daily gainers and losers (Stocks & REITs), no API key required.",
  "author": "Elton Santos",
  "homepage_url": "https://eltonmelosantos.com.br",
  "action": {
    "default_title": "US Quick Quote",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://stooq.com/*"
  ]
}
